{
  "name": "api-builder",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "typecheck": "eslint src/**/*.ts",
    "dev": "tsx watch -r tsconfig-paths/register src/server.ts",
    "test:unit": "npx dotenv-cli -e  .env.test -- vitest run --passWithNoTests __tests__/unit/**/*.test.ts",
    "pretest:integration": "docker-compose -f docker-compose-test.yml up -d --build && npx dotenv-cli -e  .env.test -- npx prisma migrate deploy",
    "test:integration": "npx dotenv-cli -e  .env.test -- vitest run --passWithNoTests __tests__/integration/**/*.test.ts",
    "posttest:integration": "docker-compose -f docker-compose-test.yml down",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "undici-types": "^7.12.0",
    "winston": "^3.17.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
