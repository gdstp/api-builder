{
  "name": "api-builder",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "typecheck": "eslint src/**/*.ts",
    "dev": "tsx watch -r tsconfig-paths/register src/server.ts",
    "prepare": "test -d .git && husky install || echo 'skip husky (no .git)'",
    "test:unit": "npx dotenv-cli -e  .env.test -- vitest run --passWithNoTests __tests__/unit/**/*.test.ts",
    "test:integration": "npx dotenv -e .env.test -- vitest run --passWithNoTests __tests__/integration/**/*.test.ts",
    "pretest:e2e": "docker compose -f docker-compose-test.yml up -d --build --wait && npx dotenv -e .env.test -- prisma db push --force-reset",
    "test:e2e": "npx dotenv -e .env.test -- vitest run --passWithNoTests __tests__/e2e/**/*.test.ts",
    "posttest:e2e": "docker compose -f docker-compose-test.yml down -v",
    "test:all:coverage": "docker compose --env-file .env.test -f docker-compose-test.yml up -d --build  --wait && npx dotenv -e .env.test -- prisma db push --force-reset && npx dotenv -e .env.test -- vitest run --coverage --passWithNoTests __tests__/**/**/*.test.ts --coverage.reporter=json-summary --coverage.reporter=text-summary --coverage.reporter=lcov && docker compose -f docker-compose-test.yml down -v"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.0.1",
    "undici-types": "^7.12.0",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
